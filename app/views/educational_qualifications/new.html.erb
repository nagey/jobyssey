<h1>New educational_qualification</h1>

<% form_for(@educational_qualification) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.collection_select :educational_level, @educational_level, :name, :name %><br />
    <%= link_to l(:add_educational_level), new_educational_level_path %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', educational_qualifications_path %>
