<h1>Editing educational_institution</h1>

<% form_for(@educational_institution) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :url %><br />
    <%= f.text_field :url %>
  </p>
  <p>
    <%= f.label :town_id %><br />
    <%= f.text_field :town_id %>
  </p>
  <p>
    <%= f.label :country_id %><br />
    <%= f.text_field :country_id %>
  </p>
  <p>
    <%= f.label :geo_position_id %><br />
    <%= f.text_field :geo_position_id %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @educational_institution %> |
<%= link_to 'Back', educational_institutions_path %>
