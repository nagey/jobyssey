<h1>New differentiator_answer</h1>

<% form_for(@differentiator_answer) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.collection_select :differentiator_question_id, @differentiator_questions, :id, :name %>
	<br>
	<% link_to l(:add_differentiator_question), new_differentiator_question_path %>
  </p>
  <p>
    <%= f.collection_select :employer_id, @employers, :id, :name %>
	<br>
	<% link_to l(:add_employer), new_employer_path %>
  </p>
  <p>
    <%= f.collection_select :user_id, @users, :id, :name %>
	<br>
	<% link_to l(:add_user), new_user_path %>
  </p>
  <p>
  <%= f.collection_select :job_posting_id, @job_postings, :id, :name %>
	<br>
	<% link_to l(:add_job_posting), new_job_posting_path %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', differentiator_answers_path %>
