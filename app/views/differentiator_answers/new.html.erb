<h1>New differentiator_answer</h1>

<% form_for(@differentiator_answer) do |f| %>
  <%= f.error_messages %>

  @job_posting = JobPosting.find :all

  <p>
    <%= f.collection_select :differentiator_question, @differentiator_question, :name, :name %>
	<br>
	<% link_to l(:add_differentiator_question), new_differentiator_question_path %>
  </p>
  <p>
    <%= f.collection_select :employer, @employer, :name, :name %>
	<br>
	<% link_to l(:add_employer), new_employer_path %>
  </p>
  <p>
    <%= f.collection_select :user, @user, :name, :name %>
	<br>
	<% link_to l(:add_user), new_user_path %>
  </p>
  <p>
  <%= f.collection_select :job_posting, @job_posting, :name, :name %>
	<br>
	<% link_to l(:add_job_posting), new_job_posting_path %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', differentiator_answers_path %>
