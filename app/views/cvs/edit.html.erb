<h1>Editing cv</h1>

<% form_for(@cv) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :attachment %><br />
    <%= f.text_field :attachment %>
  </p>
  <p>
    <%= f.label :entity_id %><br />
    <%= f.text_field :entity_id %>
  </p>
  <p>
    <%= f.label :entity_type %><br />
    <%= f.text_field :entity_type %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @cv %> |
<%= link_to 'Back', cvs_path %>
