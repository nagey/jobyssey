<h1>Editing differentiator_question</h1>

<% form_for(@differentiator_question) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :type %><br />
    <%= f.text_field :type %>
  </p>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @differentiator_question %> |
<%= link_to 'Back', differentiator_questions_path %>
