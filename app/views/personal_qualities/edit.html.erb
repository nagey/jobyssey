<h1>Editing personal_quality</h1>

<% form_for(@personal_quality) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.label :metric_id %><br />
    <%= f.text_field :metric_id %>
  </p>
  <p>
    <%= f.label :value %><br />
    <%= f.text_field :value %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @personal_quality %> |
<%= link_to 'Back', personal_qualities_path %>
