<h1>Editing personal_question</h1>

<% form_for(@personal_question) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @personal_question %> |
<%= link_to 'Back', personal_questions_path %>
