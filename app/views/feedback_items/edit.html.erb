<h1>Editing feedback_item</h1>

<% form_for(@feedback_item) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :job_application_id %><br />
    <%= f.text_field :job_application_id %>
  </p>
  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.label :on_time %><br />
    <%= f.check_box :on_time %>
  </p>
  <p>
    <%= f.label :serious %><br />
    <%= f.check_box :serious %>
  </p>
  <p>
    <%= f.label :honest %><br />
    <%= f.check_box :honest %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @feedback_item %> |
<%= link_to 'Back', feedback_items_path %>
